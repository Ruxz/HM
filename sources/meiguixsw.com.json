{
  "name": "玫瑰小说网",
  "url": "meiguixsw.com",
  "remarks": "两次搜索的间隔时间不得少于100秒",
  "version": 103,
  "search": {
    "url": "https://www.meiguixsw.com/modules/article/search.php@post->searchkey=${key}",
    "charset": "gbk",
    "list": "#nr",
    "name": "td:nth-child(1) > a",
    "author": "td:nth-child(3)",
    "detail": "td:nth-child(1) > a"
  },
  "detail": {
    "name": ".bookright > h1",
    "author": "#author > a",
    "cover": "#bookimg > img",
    "summary": "#bookintro > p",
    "status": "#count > span:nth-child(6)",
    "update": "span.new_t@match->(?<=最后更新：)(.+)",
    "lastChapter": "#newlist > ul > li:nth-child(1) > a",
    "catalog": "#reader > a"
  },
  "catalog": {
    "list": "#readerlist > ul > li",
    "orderBy": 0,
    "name": "a",
    "chapter": "a"
  },
  "chapter": {
    "content": "#content",
    "filter": ["%玫瑰小说网.+|玫瑰小说网.+最快更新.+"]
  }
}